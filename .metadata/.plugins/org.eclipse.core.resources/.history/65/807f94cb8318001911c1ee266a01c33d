import java.util.HashMap;
import java.util.Map;

public class balanced {
	
	public  boolean perfectlyBalanced(String name){
		
	Map<Character,Integer> map = new HashMap<>();
	
	map.put('x',0);
	map.put('y',0);
	
	int countx = map.get('x');
	int countx = map.get('x');
	
	for(int i =0; i<name.length(); i++){
			if(name.charAt(i) == 'x'){
				map.put('x',countx+1);
			}
	}
		return true;
	}
}
